{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.wordWrap": "on",
  "editor.fontFamily": "Camingocode",
  "editor.formatOnSave": true,
  "workbench.editor.enablePreview": false,
  "workbench.sideBar.location": "right",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "commands": [":wq"]
    },
    {
      "before": ["c"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["workbench.action.openEditorAtIndex5"]
    },
    {
      "before": ["<leader>", "6"],
      "commands": ["workbench.action.openEditorAtIndex6"]
    },
    {
      "before": ["<leader>", "7"],
      "commands": ["workbench.action.openEditorAtIndex7"]
    },
    {
      "before": ["<leader>", "8"],
      "commands": ["workbench.action.openEditorAtIndex8"]
    },
    {
      "before": ["<leader>", "9"],
      "commands": ["workbench.action.openEditorAtIndex9"]
    },
    {
      "before": ["<leader>", "0"],
      "commands": ["workbench.action.openEditorAtIndex0"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["m", "m"],
      "after": ["$", "V", "%"]
    },
    {
      "before": ["<leader>", "v", "a"],
      "after": ["g", "g", "V", "G"]
    }
  ],
  "vim.enableNeovim": true,
  "explorer.compactFolders": false,
  "timeline.excludeSources": [],
  "workbench.editor.showTabs": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "extensions.autoUpdate": false,
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "none",
  "emmet.includeLanguages": {
    "javascript": "html"
  },
  "editor.fontSize": 16,
  "workbench.colorTheme": "Verdandi Alter Customized",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "prettier.semi": false,
  "space-before-function-paren": true,
  "workbench.iconTheme": "chalice-icon-theme",
  "CSSPercent.fixedDigits": 2,
  "org.todoKeywords": [
    "NEXT",
    "TODO",
    "WAITING",
    "SOMEDAY",
    "PROJ",
    "DONE",
    "CANCELLED"
  ]
}
